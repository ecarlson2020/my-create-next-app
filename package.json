{
  "dependencies": {
    "next": "^15.5.4",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "scripts": {
    "build-staging": "NODE_ENV=staging yarn && yarn pretty && yarn lint && next build",
    "build-prod": "NODE_ENV=production yarn && yarn pretty && yarn lint && next build",
    "lint": "next lint --max-warnings 0",
    "fix": "next lint --fix && yarn pretty-fix",
    "pretty": "prettier --check \"**/*.{js,jsx,ts,tsx}\"",
    "pretty-fix": "prettier --write \"**/*.{js,jsx,ts,tsx}\"",
    "up-staging": "yarn build-staging && qscp -u out",
    "up-prod": "yarn build-prod && qscp -u out",
    "down": "website_location='/home/ecarlson10/webapps/test2.evrocamedia' && rm -rf $website_location && mv ~/out $website_location",
    "ui": "yarn && next dev -p 5001 & yarn tsc --watch && fg",
    "api": "cd api && npm i && npm run dev",
    "db": "docker-compose up -d mysql && docker-compose up -d phpmyadmin",
    "db-down": "docker-compose down -v"
  },
  "devDependencies": {
    "@types/react": "19.2.2",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "eslint": "^9.37.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "15.5.4",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-webpack-plugin": "^5.0.2",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "unimported": "^1.31.1"
  }
}
